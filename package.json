{
  "name": "service-easy-notification",
  "version": "1.0.0",
  "env": {
    "dist_main_path": "dist-server/main.js"
  },
  "scripts": {
    "init": "node initialization/entrypoint.js",
    "start": "node $npm_package_env_dist_main_path",
    "dev": "concurrently \"yarn serve\" \"yarn watch\"",
    "compile": "npm run build",
    "build": "tsc -p ./server",
    "watch": "yarn build -- --watch",
    "serve": "nodemon $npm_package_env_dist_main_path",
    "lint": "eslint --ext .js,.ts --ignore-path .gitignore . --fix",
    "pretty": "pretty-quick --staged",
    "set:env": "env-cmd -f .env",
    "git:reset": "git fetch --all && git reset --hard --recurse-submodules && git checkout master && git pull --rebase && git submodule update --rebase --remote --recursive",
    "lint-staged": "lint-staged",
    "test": "ava",
    "test:unit": "ava ./test/specs/**/*",
    "test:e2e": "ava ./test/e2e/**/*",
    "test:jest:e2e": "jest ./test/e2e/*.spec.ts ./test/e2e/**/*.spec.ts --maxWorkers=1"
  },
  "lint-staged": {
    "*.{ts,js}": "npm run lint"
  },
  "dependencies": {
    "@hapi/boom": "9.1.2",
    "axios": "0.21.1",
    "bluebird": "3.7.2",
    "carotte-amqp": "2.4.2",
    "cycle": "1.0.3",
    "knex": "0.95.6",
    "mjml": "4.9.3",
    "ms": "2.1.3",
    "multer": "1.4.2",
    "mustache": "4.2.0",
    "nodemailer": "6.6.1",
    "nodemailer-mandrill-transport": "1.2.0",
    "pg": "8.6.0",
    "shortid": "2.2.16",
    "snake-case": "3.0.4",
    "typeorm": "0.2.32",
    "winston": "3.3.3",
    "winston-daily-rotate-file": "4.5.5"
  },
  "devDependencies": {
    "@ava/babel": "1.0.1",
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@types/amqplib": "0.5.17",
    "@types/bcrypt": "5.0.0",
    "@types/bluebird": "3.5.35",
    "@types/imagemin": "7.0.0",
    "@types/imagemin-gifsicle": "7.0.0",
    "@types/imagemin-jpegtran": "5.0.0",
    "@types/jest": "26.0.23",
    "@types/jsonwebtoken": "8.5.1",
    "@types/morgan": "1.9.2",
    "@types/ms": "0.7.31",
    "@types/multer": "1.4.5",
    "@types/mustache": "4.1.1",
    "@types/node": "15.6.1",
    "@types/node-fetch": "2.5.10",
    "@types/shortid": "0.0.29",
    "@types/supertest": "2.0.11",
    "ava": "3.15.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-module-resolver": "4.1.0",
    "browser-env": "3.3.0",
    "commitlint": "12.1.4",
    "concurrently": "6.2.0",
    "env-cmd": "10.1.0",
    "eslint": "7.27.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "6.0.0",
    "jest": "27.0.1",
    "lint-staged": "11.0.0",
    "nodemon": "2.0.7",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.0",
    "pretty-quick": "3.1.0",
    "require-extension-hooks": "0.3.3",
    "require-extension-hooks-babel": "1.0.0",
    "supertest": "6.1.3",
    "ts-jest": "27.0.1",
    "typescript": "4.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s pretty",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
