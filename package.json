{
  "name": "service-easy-notification",
  "version": "1.0.0",
  "env": {
    "dist_main_path": "dist-server/main.js"
  },
  "scripts": {
    "init": "node initialization/entrypoint.js",
    "start": "node $npm_package_env_dist_main_path",
    "start-nuxt": "nuxt start",
    "dev": "concurrently \"yarn serve\" \"yarn watch\"",
    "compile": "npm run build && npm run build-nuxt",
    "build": "tsc -p ./server",
    "build-nuxt": "nuxt build",
    "watch": "yarn build -- --watch",
    "serve": "nodemon $npm_package_env_dist_main_path",
    "lint": "eslint --ext .js,.vue,.ts --ignore-path .gitignore . --fix",
    "pretty": "pretty-quick --staged",
    "set:env": "env-cmd -f .env",
    "git:reset": "git fetch --all && git reset --hard --recurse-submodules && git checkout master && git pull --rebase && git submodule update --rebase --remote --recursive",
    "lint-staged": "lint-staged",
    "test": "ava",
    "test:unit": "ava ./test/specs/**/*",
    "test:e2e": "ava ./test/e2e/**/*",
    "test:jest:e2e": "jest ./test/e2e/*.spec.ts ./test/e2e/**/*.spec.ts --maxWorkers=1"
  },
  "lint-staged": {
    "*.{ts,js,vue}": "npm run lint",
    "*.{css,vue}": "stylelint"
  },
  "dependencies": {
    "@hapi/boom": "^9.1.0",
    "axios": "^0.21.0",
    "bluebird": "^3.7.2",
    "carotte-amqp": "^2.4.1-beta.0",
    "cycle": "^1.0.3",
    "knex": "^0.21.0",
    "mjml": "^4.8.1",
    "ms": "^2.1.2",
    "multer": "^1.4.2",
    "mustache": "^4.1.0",
    "nodemailer": "^6.5.0",
    "nodemailer-mandrill-transport": "^1.2.0",
    "pg": "^8.0.3",
    "shortid": "^2.2.15",
    "snake-case": "^3.0.3",
    "typeorm": "^0.2.24",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nuxt/typescript-build": "^0.6.6",
    "@nuxt/typescript-runtime": "^0.4.6",
    "@nuxtjs/eslint-config-typescript": "^1.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/stylelint-module": "^3.1.0",
    "@nuxtjs/vuetify": "^1.0.0",
    "@types/amqplib": "^0.5.13",
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.32",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/imagemin": "^7.0.0",
    "@types/imagemin-gifsicle": "^7.0.0",
    "@types/imagemin-jpegtran": "^5.0.0",
    "@types/jest": "^26.0.10",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/morgan": "^1.9.1",
    "@types/ms": "^0.7.31",
    "@types/multer": "^1.4.5",
    "@types/mustache": "^4.1.1",
    "@types/node": "^13.13.4",
    "@types/node-fetch": "^2.5.7",
    "@types/shortid": "0.0.29",
    "@types/supertest": "^2.0.10",
    "ava": "^3.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "browser-env": "^3.2.5",
    "commitlint": "^11.0.0",
    "concurrently": "^5.3.0",
    "env-cmd": "^10.1.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-nuxt": "^1.0.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.0.0",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "prettier": "2.2.1",
    "pretty-quick": "3.0.2",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^1.0.0",
    "require-extension-hooks-vue": "^2.0.0",
    "stylelint": "^10.1.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.3.0",
    "typescript": "^3.8.3",
    "vue-template-compiler": "^2.6.12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s pretty",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
